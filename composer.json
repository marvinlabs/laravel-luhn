{
    "name": "marvinlabs/laravel-luhn",
    "description": "Luhn algorithm for Laravel",
    "keywords": [
        "laravel",
        "luhn",
        "credit card",
        "siren",
        "siret"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Vincent Mimoun-Prat",
            "email": "contact@vincentprat.info",
            "homepage": "https://vincentprat.info",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/support": "^5.5|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0|^9.0|^10.5",
        "mockery/mockery": ">=0.9",
        "orchestra/testbench": "^3.6|^4.0|^5.0|^6.0|^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "MarvinLabs\\Luhn\\": "src/Luhn"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MarvinLabs\\Luhn\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MarvinLabs\\Luhn\\LuhnServiceProvider"
            ],
            "aliases": {
                "Luhn": "MarvinLabs\\Luhn\\Facades\\Luhn"
            }
        }
    },
    "minimum-stability": "stable"
}
